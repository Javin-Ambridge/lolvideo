<div class="row header-margin-top">
	<div class="small-12 large-12 columns left-dashboard">
		{{#if queryImplemented}}
			<div class="ld-options">
				<div class="ld-regions">
					{{#power-select
			      options=regions
			      selected=region
			      onchange=(action "selectedRegion")
			      searchField='name'
			      as |reg|
			    }}
			      {{reg.name}}
			    {{/power-select}}
				</div>
				<div class="ld-regions">
					{{#power-select
			      options=years
			      selected=currYear
			      onchange=(action "selectedYear")
			      as |yr|
			    }}
			      {{yr}}
			    {{/power-select}}
				</div>
			</div>
		{{/if}}
		{{#if bigFilm}}
			<div class="ld-title1">
				Top Rated Games
			</div>
			{{#unless bigFilm.loaded}}
				<a class="gallerypic" title="" {{action 'bigFilmClick'}}>
				  <img src={{youtube-thumbnail bigFilm.id}} style="width: 100%; height: inherit; max-height: 550px;" alt="[Gallery Photo]" class="pic" id="bigfilm-container"/>
				  <span class="zoom-icon" style={{zoomIconBig}}>
				      <img src="/images/play_square.svg" width="85" height="85" alt="Zoom">
				  </span>
				</a>
			{{else}}
				<div class="big-film-container" style={{bigFilmStyle}}>
					{{ember-youtube
					playerVars=videoVars
			    ytid=bigFilm.id
			    volume=60
			    startSeconds=5
			    suggestedQuality='default'
			 
			    showControls=false
			    showTime=false
			    showProgress=false
			    showDebug=false
			 		
			    width=bigFilmWidth
			    height=bigFilmHeight

			    delegate=this
			    delegate-as="emberYoutube"
			 
			    playing="ytPlaying"
			    paused="ytPaused"
			    ended="ytEnded"
			    buffering="ytBuffering"}}
			  </div>
			{{/unless}}
	    	<div class="bigfilm-name">{{bigFilm.name}}</div>
	    	{{bottom-video large=true count=bigFilm.count name=bigFilm.name idKey=bigFilm.key awesome=true}}
		{{else}}
			<div class="loader-top" id="fountainG">
				<div id="fountainG_1" class="fountainG"></div>
				<div id="fountainG_2" class="fountainG"></div>
				<div id="fountainG_3" class="fountainG"></div>
				<div id="fountainG_4" class="fountainG"></div>
				<div id="fountainG_5" class="fountainG"></div>
				<div id="fountainG_6" class="fountainG"></div>
				<div id="fountainG_7" class="fountainG"></div>
				<div id="fountainG_8" class="fountainG"></div>
			</div>
		{{/if}}
    	<div class="smaller-video-container">
	    	{{#if awesome}}
		    	<div class="row">
			    	{{#each awesome as |vid ind|}}
			    		<div class="small-12 medium-4 columns small-video-area" style={{containerSizeSmall}}>
			    			{{#unless vid.loaded}}
									<a class="gallerypic" title="" {{action 'vidClick' 'awesome' ind}}>
									  <img src={{youtube-thumbnail vid.id}} style="width: 100%;" alt="[Gallery Photo]" class="pic" id="awesome-container"/>
									  <span class="zoom-icon small" style={{zoomIconSmall}}>
									      <img src="/images/play_square.svg" width="85" height="85" alt="Zoom">
									  </span>
									</a>
								{{else}}
									<div class="small-vid-container" style={{normalFilmStyle}}>
										{{ember-youtube
								    ytid=vid.id
										playerVars=videoVars
								    startSeconds=5
								    suggestedQuality='default'
								    volume=100
								 
								    showControls=false
								    showTime=false
								    showProgress=false
								    showDebug=false
								 		
								    width=awesomeSAndClowWidth
								    height=awesomeSAndClowHeight

								    delegate=this
								    delegate-as="emberYoutube"}}
								  </div>
								{{/unless}}
						    <span class="small-size">{{vid.name}}</span>
		    				{{bottom-video small=true count=vid.awesomeN name=vid.name idKey=vid.key}}
		    			</div>
			    	{{/each}}
				</div>
		    	<div class="load-more-button">
		    		<a class="large button" {{action 'loadmore' 'awesome'}}>Load More</a>
		    		{{#link-to 'awesome' class="large button view-all"}}View All{{/link-to}}
		    	</div>
	    	{{/if}}
	    </div>
	    <div class="spacer-container margin-top-1 margin-bottom-3">
		    <div class="or-spacer">
				  <div class="mask"></div>
				  <span><i>or</i></span>
				</div>
	    </div>
		{{#if fiesta}}
			<div class="ld-title1">
				Fun Games
			</div>
			<div class="smaller-video-container">
				<div class="row">
			    	{{#each fiesta as |vid ind|}}
			    		<div class="small-12 medium-4 columns small-video-area" style={{containerSizeSmall}}>
			    			{{#unless vid.loaded}}
									<a class="gallerypic" title="" {{action 'vidClick' 'fiesta' ind}}>
									  <img src={{youtube-thumbnail vid.id}} style="width: 100%;" alt="[Gallery Photo]" class="pic" />
									  <span class="zoom-icon small" style={{zoomIconSmall}}>
									      <img src="/images/play_square.svg" width="85" height="85" alt="Zoom">
									  </span>
									</a>
								{{else}}
									<div class="small-vid-container" style={{normalFilmStyle}}>
										{{ember-youtube
									    ytid=vid.id
											playerVars=videoVars
									    startSeconds=5
									    suggestedQuality='default'
									    volume=100
									 
									    showControls=false
									    showTime=false
									    showProgress=false
									    showDebug=false
									 		
									    width=awesomeSAndClowWidth
									    height=awesomeSAndClowHeight

									    delegate=this
									    delegate-as="emberYoutube"}}
									 </div>
								{{/unless}}
						    <span class="small-size">{{vid.name}}</span>
		    				{{bottom-video small=true count=vid.funstompN idKey=vid.key fun=true}}
						  </div>
			    	{{/each}}
			    </div>
		    	<div class="load-more-button">
		    		<a class="large button" {{action 'loadmore' 'fiesta'}}>Load More</a>
		    		{{#link-to 'fun' class="large button view-all"}}View All{{/link-to}}
		    	</div>
			</div>
		{{else}}
			<div class="loader-top2" id="fountainG">
				<div id="fountainG_1" class="fountainG"></div>
				<div id="fountainG_2" class="fountainG"></div>
				<div id="fountainG_3" class="fountainG"></div>
				<div id="fountainG_4" class="fountainG"></div>
				<div id="fountainG_5" class="fountainG"></div>
				<div id="fountainG_6" class="fountainG"></div>
				<div id="fountainG_7" class="fountainG"></div>
				<div id="fountainG_8" class="fountainG"></div>
			</div>
		{{/if}}
	</div>
	<div class="small-12 columns right-dashboard margin-bottom-2">
		<br>
		{{#if queryImplemented}}
			<div class="rd-options">
				{{#power-select
		      options=regionsR
		      selected=regionR
		      onchange=(action "selectedRegion")
		      searchField='name'
		      as |reg|
		    }}
		      {{reg.name}}
		    {{/power-select}}
				{{#power-select
		      options=yearsR
		      selected=currYearR
		      onchange=(action "selectedYear")
		      as |yr|
		    }}
		      {{yr}}
		    {{/power-select}}
			</div>
		{{/if}}
		{{#if newest}}
			<div class="ld-title1">
				Most Recent Games
			</div>
			<div class="recent-videos-container">
				<div class="row">
					{{#each newest as |vid ind|}}
						<div class="small-12 medium-4 columns small-video-area medium" style={{containerSizeMed}}>
							{{#unless vid.loaded}}
								<a class="gallerypic" title="" {{action 'vidClick' 'newest' ind}}>
								  <img src={{youtube-thumbnail vid.id}} style="width: 100%;" alt="[Gallery Photo]" class="pic" id="recent-container"/>
								  <span class="zoom-icon medium" style={{zoomIconMed}}>
								      <img src="/images/play_square.svg" width="85" height="85" alt="Zoom">
								  </span>
								</a>
							{{else}}
								<div class="medium-vid-container" style={{mediumFilmStyle}}>
									{{ember-youtube
								    ytid=vid.id
										playerVars=videoVars
								    startSeconds=5
								    suggestedQuality='default'
								    volume=100
								 
								    showControls=false
								    showTime=false
								    showProgress=false
								    showDebug=false
								 		
								    width=mostRecentWidth
								    height=mostRecentHeight

								    delegate=this
								    delegate-as="emberYoutube"}}
								</div>
							{{/unless}}
					    <span class="small-size">{{vid.name}}</span>
							{{bottom-video medium=true count=vid.awesomeN idKey=vid.key name=vid.name}}
				  	</div>
					{{/each}}
				</div>
		    	<div class="load-more-button">
		    		<a class="large button" {{action 'loadmore' 'newest'}}>Load More</a>
		    		{{#link-to 'newest' class="large button view-all"}}View All{{/link-to}}
		    	</div>
			</div>
		{{else}}
			<div class="loader-top3" id="fountainG">
				<div id="fountainG_1" class="fountainG"></div>
				<div id="fountainG_2" class="fountainG"></div>
				<div id="fountainG_3" class="fountainG"></div>
				<div id="fountainG_4" class="fountainG"></div>
				<div id="fountainG_5" class="fountainG"></div>
				<div id="fountainG_6" class="fountainG"></div>
				<div id="fountainG_7" class="fountainG"></div>
				<div id="fountainG_8" class="fountainG"></div>
			</div>
		{{/if}}
	</div>
</div>